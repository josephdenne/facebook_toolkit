<?xml version="1.1" encoding="UTF-8"?>
<extension id="facebook_toolkit" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">
	<name>Facebook Toolkit</name>
	<description>Provide baseline functionality for Facebook applications and Facebook authenticating sites</description>
	<repo type="github">https://github.com/josephdenne/facebook_toolkit</repo>
	<url type="discuss">http://getsymphony.com/discuss/thread/80762/</url>
	<types>
		<type>Events</type>
		<type>Membership</type>
		<type>Third Party Integration</type>
	</types>
	<authors>
		<author>
			<name github="josephdenne" symphony="Joseph">Joseph Denne</name>
			<website>http://josephdenne.com/</website>
			<email>mail@josephdenne.com</email>
		</author>
		<author>
			<name github="henrysingleton" symphony="Henry">Henry Singleton</name>
			<website>http://henrysingleton.com/</website>
			<email>me@henrysingleton.com</email>
		</author>
	</authors>
	<releases>
		<release version="1.1" date="2012-10-1" min="2.3">
			- Symphony 2.3 compatibility
			- Adds Facebook API key for access via frontend page params
			- Updates Facebook PHP SDK to 3.2.0, and includes it as a submodule
			- Documentation updates
		</release>
		<release version="1.0" date="2011-11-19" min="2.2.2" max="2.2.5">
			- Initial release
		</release>
	</releases>
</extension>